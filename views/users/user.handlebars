<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Welcome, {{ user.name }}</h1>
    <br>
    <h3 class="lead">{{ user.email }}</h3>
  </div>
</div>


<div class="card text-center">
  <div class="card-header">
    Your Orders
  </div>
  {{#each user.orders}}
    <ul>
        <li>Order placed: {{this.orderDateTime}}</li>
        <li>Total: {{this.total}}</li>
    </ul>
    <hr>
  {{/each}}
